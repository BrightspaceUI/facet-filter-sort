{
  "name": "Facet Filter Sort",
  "source_dir": "lang",
  "source_match": "en\\.json$",
  "output_file_path": "lang/%LANG%.js",
  "parser_plugin": {
		"plugin": "parse_js"
	},
  "output_lang_rewrite": [
    "ar-sa ar",
    "cy-gb cy",
    "da-dk da",
    "de-de de",
    "es-mx es",
    "fr-ca fr",
    "ja-jp ja",
    "ko-kr ko",
    "nl-nl nl",
    "pt-br pt",
    "sv-se sv",
    "tr-tr tr"
  ]
}
