{
  "description": "Collection of (search) facet, filter and sort elements",
  "repository": {
    "type": "git",
    "url": "https://github.com/BrightspaceUI/facet-filter-sort.git"
  },
  "name": "d2l-facet-filter-sort",
  "scripts": {
    "analyze": "wca analyze components/**/*.js --format json --outFile custom-elements.json",
    "lint": "npm run lint:wc && npm run lint:js",
    "lint:js": "eslint . --ext .js,.html test/**/*.js test/**/*.html demo/**/*.js demo/**/*.html",
    "lint:wc": "polymer lint -i \"./*\" \"test\" \"demo\" \"components\"",
    "test:polymer:local": "polymer test --skip-plugin sauce",
    "test:polymer:sauce": "polymer test --skip-plugin local",
    "test": "npm run lint && npm run test:polymer:local"
  },
  "author": "D2L Corporation",
  "license": "Apache-2.0",
  "devDependencies": {
    "@polymer/iron-test-helpers": "^3.0.0",
    "@webcomponents/webcomponentsjs": "^2.2.4",
    "babel-eslint": "^10.0.1",
    "chromedriver": "^2.40.0",
    "eslint": "^4.19.1",
    "eslint-config-brightspace": "^0.4.0",
    "eslint-plugin-html": "^4.0.5",
    "frau-ci": "^1.33.2",
    "polymer-cli": "^1.9.5",
    "sauce-connect-launcher": "^1.2.4",
    "wct-browser-legacy": "^1.0.1",
    "web-component-analyzer": "^1"
  },
  "version": "3.6.0",
  "resolutions": {
    "inherits": "2.0.3",
    "samsam": "1.1.3",
    "supports-color": "3.1.2",
    "type-detect": "1.0.0"
  },
  "dependencies": {
    "@brightspace-ui/core": "^1.43.3",
    "d2l-localize-behavior": "BrightspaceUI/localize-behavior#semver:^2",
    "@brightspace-ui-labs/multi-select": "BrightspaceUILabs/multi-select#semver:^3",
    "d2l-polymer-behaviors": "Brightspace/d2l-polymer-behaviors-ui#semver:^2",
    "d2l-typography": "BrightspaceUI/typography#semver:^7",
    "lit-element": "^2",
    "@polymer/polymer": "^3.0.0"
  }
}
